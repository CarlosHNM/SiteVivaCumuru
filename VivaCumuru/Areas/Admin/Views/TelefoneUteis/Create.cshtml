@model VivaCumuru.Models.TelefoneUteis

@{
    ViewData["Title"] = "Create";
}
<h3>Criar Telefones</h3>
<hr />
<div class="row">
    <div class="col-md-12">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TelNome" class="control-label"></label>
                <input asp-for="TelNome" class="form-control" />
                <span asp-validation-for="TelNome" class="text-danger"></span>
            </div>
             <div class="form-group">
            <label asp-for="TelNum" class="control-label"></label>
            <input asp-for="TelNum" class="form-control" />
            <span asp-validation-for="TelNum" class="text-danger"></span>
            </div>
            <hr />
            <div class="form-group">
                <input type="submit" value="Criar Telefone" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-outline-info">Retornar</a>
            </div>
        </form>
    </div>
</div>
